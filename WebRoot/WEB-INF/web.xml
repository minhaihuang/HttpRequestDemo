<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>DispatcherDemoServlet</servlet-name>
    <servlet-class>DispatcherDemoServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>DispatcherDemoServlet02</servlet-name>
    <servlet-class>DispatcherDemoServlet02</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>WebObjServlet</servlet-name>
    <servlet-class>minhaihuang.request.webObj.WebObjServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>WebObj02Servlet</servlet-name>
    <servlet-class>minhaihuang.request.webObj.WebObj02Servlet</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>DispatcherDemoServlet</servlet-name>
    <url-pattern>/DispatcherDemo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DispatcherDemoServlet02</servlet-name>
    <url-pattern>/DispatcherDemo02</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>WebObjServlet</servlet-name>
    <url-pattern>/WebObj</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>WebObj02Servlet</servlet-name>
    <url-pattern>/WebObj02</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
